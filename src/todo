let counter = -1;
{characters.map((char) => {
    counter++;
    if (char === {}) {
        return <button onClick={saveChar(JSON.parse(JSON.stringify(counter)), character)}>(empty)</button>;
    }
    return <button onClick={saveChar(JSON.parse(JSON.stringify(counter)), character)}>{character.name}</button>;
});}












